	<oad:uod rdf:about="uod/CD1700000521">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disegni di legge</rdfs:label>
		<dc:title>Disegni di legge</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/series"/>
		<dcterms:isPartOf rdf:resource="uod/CD1700000504"/>
		<dcterms:provenance rdf:resource="organization/camera"/>
		<oad:referenceCode>00200.00040.00007.00002</oad:referenceCode>
