	<oad:uod rdf:about="uod/IT-ER-IBC-037006-004-104">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carte Lino Hanau</rdfs:label>
		<dc:title>Carte Lino Hanau</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037006-004"/>
