		<dc:title>Archivio del Consorzio regionale fra gli istituti autonomi per le case popolari dell’Emilia-Romagna</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
