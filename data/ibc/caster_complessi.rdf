		<dc:title>Archivio dell'Ufficio notizie per le famiglie dei militari - Sezione di Bologna</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037006-002"/>
