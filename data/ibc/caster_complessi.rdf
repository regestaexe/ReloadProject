		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio del Centro professionale di Cento</rdfs:label>
		<dc:title>Archivio del Centro professionale di Cento</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037006-011"/>
