		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carte Enzo Gatti</rdfs:label>
		<dc:title>Carte Enzo Gatti</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
