	<oad:uod rdf:about="uod/IT-ER-IBC-037006-011-037">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio della Gioventù italiana - Ufficio provinciale di Bologna</rdfs:label>
		<dc:title>Archivio della Gioventù italiana - Ufficio provinciale di Bologna</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
