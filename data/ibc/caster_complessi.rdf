		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio del Patronato scolastico di Casalecchio di Reno</rdfs:label>
		<dc:title>Archivio del Patronato scolastico di Casalecchio di Reno</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037011-001"/>
