		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio degli Utenti motori agricoli di Bologna</rdfs:label>
		<dc:title>Archivio degli Utenti motori agricoli di Bologna</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037006-011"/>
