		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio dell'Azienda regionale per la gestione del centro elettronico dell'Emilia-Romagna</rdfs:label>
		<dc:title>Archivio dell'Azienda regionale per la gestione del centro elettronico dell'Emilia-Romagna</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
