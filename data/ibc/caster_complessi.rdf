		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio del Provveditorato regionale alle opere pubbliche per l'Emilia</rdfs:label>
		<dc:title>Archivio del Provveditorato regionale alle opere pubbliche per l'Emilia</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
