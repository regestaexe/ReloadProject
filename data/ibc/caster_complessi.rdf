	<oad:uod rdf:about="uod/IT-ER-IBC-037046-001-005">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Archivio dell'Ufficio di conciliazione di Ozzano dell'Emilia</rdfs:label>
		<dc:title>Archivio dell'Ufficio di conciliazione di Ozzano dell'Emilia</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
		<dcterms:provenance rdf:resource="organization/ibc"/>
		<oad:has_custody rdf:resource="custody/037046-001"/>
