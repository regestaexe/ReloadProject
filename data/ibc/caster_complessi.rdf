	<oad:uod rdf:about="uod/IT-ER-IBC-037032-001-084">
		<rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carte Gualandi</rdfs:label>
		<dc:title>Carte Gualandi</dc:title>
		<oad:has_level rdf:resource="levelOfDescription/fonds"/>
